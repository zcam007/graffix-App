{
  "name": "GraffixApp",
  "version": "1.2.2",
  "description": "An app that shows the projects of students.",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "build": "build --x64 --mac",
    "ship": "build --x64 --mac -p always"
  },
  "build": {
    "appId": "graffixApp",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "repository": "https://github.com/zcam007/graffix-App.git",
  "keywords": [
    "Graffix",
    "App",
    "Calstatela",
    "Student Union"
  ],
  "author": "Chandu & Stewart",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.6",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "csv": "^5.1.1",
    "csvtojson": "^2.0.8",
    "dialog": "^0.3.1",
    "electron-log": "^3.0.6",
    "electron-simple-updater": "^1.5.0",
    "electron-store": "^4.0.0",
    "electron-updater": "^4.0.6",
    "export-to-csv": "^0.2.1",
    "https": "^1.0.0",
    "json2csv": "^4.3.5",
    "json2xls": "^0.1.2",
    "mysql": "^2.16.0",
    "path": "^0.12.7",
    "remote": "^0.2.6",
    "request": "^2.88.0",
    "update-electron-app": "^1.3.0"
  }
}
